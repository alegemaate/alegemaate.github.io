{
  "compilerOptions": {
    "jsx": "preserve",
    "lib": ["dom", "esnext"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "sourceMap": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", ".eslintrc.js", "gatsby-config.js"]
}
